<div class="docs-sidebar">
  <nav class="docs-nav">
    <% navigation.sections.forEach(section => { %>
      <div class="docs-nav-section">
        <h4><%= section.title %></h4>
        <% section.links.forEach(link => { %>
          <a href="<%= link.href %>" class="docs-nav-link"><%= link.label %></a>
        <% }); %>
      </div>
    <% }); %>
  </nav>
</div>